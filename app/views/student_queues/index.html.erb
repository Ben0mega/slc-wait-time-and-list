<h1>Queue</h1>
<table id="queue", border = "1">
<tr>
  <th>Number</th>
  <th>Waiting</th>
  <th>Name</th>
  <th>Course</th>
</tr>

<tr>
<% @queueentries.each do |entry| %>
      <tr id="q_row">
        <td></td>
        <td><%= time_diff(entry.created_at, Time.now) %></td>
        <td><%= %Q(#{entry.student.first_name} #{entry.student.last_name}) %></td>
        <td><%= entry.course %></td>
        <% end %>
      </tr>
</tr>
</table>